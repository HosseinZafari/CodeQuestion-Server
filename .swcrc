{
    "jsc": {
        "parser": {
          "syntax": "typescript",
          "decorators": true,
          "dynamicImport": true
        },
        "transform": {
          "legacyDecorator": true,
          "decoratorMetadata": true
        },
        "target": "es5",
        "keepClassNames": true,
        "loose": true
      },
      "module": {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
      }

    
  }